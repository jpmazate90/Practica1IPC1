contructor(nombrePokemon,vidaPokemon, nivelPokemon,imagenASCII,estadoPokemon, estadoPokemonPelea)
	nombrePokemon = tNombrePokemon
	nivelPokemon = tNivelPokemon
	vidaPokemon = tVidaPokemon
	imagenASCII = tImagenASCII
	estadoPokemon = tEstadoPokemon
	estadoPokemonPelea= tEstadoPokemonPelea
	cuentacomidas=0
	cuentaactividades =0
	cantidadPokemonVivos=0
  cantidadPokemonEnemigosDerrotados=0
  experienciaPokemon = 0
  cantidadVecesDesmayado=0
  cantidadVecesHuido=0
