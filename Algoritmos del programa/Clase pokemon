metodo int atacar(nombrePokemon, vidaPokemon, numPokemonActivo)
	llamar al metodo estadoPokemonPelea
	var numAleatorio1= numero aleatorio entre 0 y 20(nivelPokemon+1)
	var daño = numAleatorio1
	escribir(nombrePokemon	 + "ha atacado")
	escribir(nombrePokemon+ "ha hecho un daño de "+daño)
	devolver daño
fin metodo atacar
	
metodo huir()
	String huir="R"
	estadoPokemonPelea= "huyo"
	cantidadVecesHuida++
	regresar huir;
fin metodo huir


metodo cambiarEstadoPokemonPelea(var tEstadoPokemonPelea)
			estadoPokemonPelea= TEstadoPokemonPelea
			regresar estadoPokemonPelea
fin metodo cambiar estadoPokemonPelea()

metodo recibirDaño( var tDaño)
	vidaPokemon-= tDaño;
	escribir(nombrePokemon+ " recibio un golpe de "+tDaño)
	escribir("Ahora tiene de salud: "+vidaPokemon)
	si(vidaPokemon<=100*0.25*(nivelPokemon+1)
		escribir("El Pokemon se ha desmayado")
		cantidadVecesDesmayado++
		estadoPokemonPelea= "desmayado"
		
	fin si
	si(vidaPokemon<=0)
		escribir("El pokemon se ha muerto")
		vidaPokemon=0
		estadoPokemonPelea="desmayado"
		estadoPokemon="muerto;
	fin si

			

metodo comer(tNombreComida, tTipoComida, tSaludMasOMenos, tVidaPokemon)
	si(tTipoComida==saludable)
		escribir(nombrePokemon"esta a punto de comer "+tNombreComida+" esta comida es saludable")
		escribir("nombrePokemon+" se curara "+tSaludMasOMenos+" puntos de salud")
		si(vidaPokemon+saludMasOMenos>100*(nivel+1))
			vidaPokemon= 100*(nivel+1)
			escribir("La vida del pokemon se ha regenerado al maximo")
		sino
			vidaPokemon+=saludMasOMenos
			escribir("la vida del pokemon ahora es: "+vidaPokemon) 	
	

	si (tipocomida==no saludable)
		escribir("El pokemon esta a punto de comer "+nombreComida+" esta comida NO es saludable")
			escribir("El pokemon perdera "+tSaludMasOMenos+ " puntos de salud")
			si(vidaPokemon+tSaludMasOMenos<=100*0.25*(nivelPokemon+1)) y vidaPokemon+tSaludMasOMenos>0)
				escribir("El pokemon se ha desmayado")
				cantidadVecesDesmayado++
				estadoPokemonPelea= desmayado
			si(vidaPokemon+TSaludMasOMenos<=0)
				escribir("El pokemon esta muerto, su salud es 0")
				vidaPokemon=0
				estadoPokemon=muerto
				estadoPokemonPelea="desmayado"
			fin si
			llamar al metodo de guardarHistorialDeComidas (clase pokemon)
fin metodo comer


metodo actividad(tNombreActividad, tTipoActividad, tSaludMasOMenos)
	si(tTipoActividad==saludable)
		escribir("El pokemon esta a punto de realizar una actividad "+tNombreActividad+" esta actividad es "+tTipoActividad)
		escribir(nombrePokemon+ " se curara "+tSaludMasOMenos+" puntos de salud")
		si(vidaPokemon+tSaludMasOMenos>100*(nivelPokemon+1))
			vidaPokemon= 100*(nivelPokemo+1)
			escribir("La vida del pokemon se ha regenerado al maximo")
		sino
			vidaPokemon+=tSaludMasOMenos
			escribir("la vida del pokemon ahora es: "+vidaPokemon) 
		llamar al metodo de guardarHistorialDeActividades (clase Actividades)
	si (tTipoActividad==no saludable)
			escribir(nombrePokemon+ " esta a punto de realizar una actividad "+tNombreActividad+" esta actividad es"+tTipoActividad)
			escribir(nombrePokemon+ " perdera "+tSaludMasOMenos+ " puntos de salud")
			si(vidaPokemon+tSaludMasOMenos<=100*0.25*(nivelPokemo+1)) && vidaPokemon+tSaludMasOMenos>0)
				escribir("El pokemon se ha desmayado")
				estadoPokemon= desmayado
			fin si
			si(vidaPokemon+tSaludMasOMenos<=0)
				escribir("El pokemon esta muerto, su salud es 0")
				vidaPokemon=0
				estadoPokemon=muerto
				estadoPokemonPelea=desmayado
		  fin si
			llamar al metodo de guardarHistorialDeActividades (clase Actividades)
fin metodo actividad


metodo subirDeNivel()
	var subirDeNivel
	var tempVerificarNivel
		subirDeNivel=experienciaPokemon%100
		tempVerificarNivel=(experienciaPokemon-subirDeNivel)/100
		si(tempVerificarNivel>nivelPokemon)
			pokemon[i].nivelPokemon=tempVerificarNivel
			escribir("El nivel de: "+nombrePokemon+" ahora es: "+nivelPokemon)
		fin si
  fin desde
fin metodo subirDeNivel

metodo recibirExperiencia(var experienciaRecibida)
				experienciaPokemon+=experienciaRecibida;
        escribir(nombrePokemon+" recibio experiencia: "+experienciaRecibida);
        escribir("Ahora tiene "+experienciaPokemon+ " puntos de experiencia");
        subirDeNivel();
fin metodo recibirExperiencia


