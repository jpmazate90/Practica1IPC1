metodo guardarHistorialComidas(tNombreComida, tTipoComida, tDarOQuitarSalud)
	historialComidas[cuentaComidas].nombreComida= tNombreComida
	historialComidas[cuentaComidas].tipoComida= tTipoComida
	historialComidas[cuentaComidas].darOQuitarSalud = tDarOQuitarSalud
	cuentaComidas++
fin guardarHistorialComidas

metodo mostrarHistorialComidasDescendentes()
	si(historialComidas[0]==null)
	finsi
	sino
		desde(i=0;i<=cuentaComidas;i++)
		escribir("El pokemon :"+nombrePokemon+ " ha ingerido: "+nombreComida)
		escribir("El tipo de comida es: "+tipoComida)
		escribir("Puntos de salud dados o quitados, dependiendo del simbolo es: "+darOQuitarSalud)
fin metodo mostarHistorialComidas

metodo mostrarHistorialComidasAscendentes()
	si(historialComidas[0]==null)
	finsi
	sino
		desde(i=cuentaComidas;i>=0;i--)
		escribir("El pokemon :"+nombrePokemon+ " ha ingerido: "+nombreComida)
		escribir("El tipo de comida es: "+tipoComida)
		escribir("Puntos de salud dados o quitados, dependiendo del simbolo es: "+darOQuitarSalud)
fin metodo mostrarHistorialComidasAscendentes


metodo guardarHistorialActividades( tDarOQuitarSalud, tTipoActividad, tNombreActividad)
	historialActividades[cuentaActividades].nombreActividad= tNombreActividad
	historialActividades[cuentaActividades].tipoActividad= tTipoActividad
	historialActividades[cuentaActividades].tDarOQuitarSalud = darOQuitarSalud
	cuentaActividades++
fin guardarHistorialComidas

metodo mostrarHistorialActividadesDescendentes
	si(historialActividades[0]==null=
	finsi
	sino
		desde(i=0; i<=cuentaActividades;i++)
		escribir("El pokemon :"+nombrePokemon+ " ha hecho una actividad: "+nombreActividad)
		escribir("El tipo de actividad es: "+tipoActividad)
		escribir("Puntos de salud dados o quitados, dependiendo del simbolo es: "+darOQuitarSalud)
fin mostrarHistorialActividadesDescendentes

metodo mostrarHistorialActividadesAscendentes
	si(historialActividades[0]==null)
	finsi
	sino
		desde(i=cuentaActividades; i<=0.lenght;i--)
		escribir("El pokemon :"+nombrePokemon+ " ha hecho una actividad: "+nombreActividad)
		escribir("El tipo de actividad es: "+tipoActividad)
		escribir("Puntos de salud dados o quitados, dependiendo del simbolo es: "+darOQuitarSalud)
fin mostrarHistorialActividadesAscendentes

metodo contarComidasBuenas
	var cantidadComidaBuena=0
	desde(var i=0;i<cuentaComdias;i++)
		si(historialComidas[i].gettTipoComida==saludable)
			cantidadComidaBuena++
		finsi
		devolver cantidadComidaBuena

fin metodo contarComidasBuenas		

metodo contarComidaMala	
	var cantidadComidaMala=0
	desde(var i=0;i<cuentaComidas;i++)
		si(historialCOmidas[i].getTipoComida==no saludable)
			cantidadComidaMala++
		fin si
	devolve cantidadComidaMala
fin metodo contarComidamala

